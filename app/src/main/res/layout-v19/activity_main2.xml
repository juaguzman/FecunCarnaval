<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondofinal01">

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/imgBtnA"
        android:background="@drawable/btnafinal"
        android:contentDescription="@string/btn"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="33dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/imgBtnF"
        android:background="@drawable/btnffinal"
        android:contentDescription="@string/btnfm"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="31dp"
        android:layout_above="@+id/imgBtnA"
        android:layout_alignStart="@+id/imgBtnA" />


</RelativeLayout>