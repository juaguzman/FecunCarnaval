<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="example.juancarlos.com.fecuncarnaval.MainFamActivity"
    android:background="@drawable/fondof">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout">

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn28"
            android:layout_row="0"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f28"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn29"
            android:layout_row="0"
            android:layout_column="2"
            android:contentDescription="@string/btn"
            android:background="@drawable/f29"
            android:layout_marginRight="5dp"
            android:contextClickable="true" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn30"
            android:layout_row="1"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f30"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn31"
            android:layout_row="1"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f31"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn2"
            android:layout_row="2"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f2"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn3"
            android:layout_row="2"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f3"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn4"
            android:layout_row="3"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f4"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtn5"
            android:layout_row="3"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/f5"
            android:layout_marginRight="5dp" />

    </GridLayout>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="84dp"
        android:id="@+id/imgBtn6"
        android:background="@drawable/f6"
        android:layout_alignTop="@+id/gridLayout"
        android:layout_alignParentStart="true"
        android:layout_marginTop="340dp"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btngrande01"
        android:id="@+id/imgbtnCfp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnAfp"
        android:background="@drawable/agenda"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnNfp"
        android:background="@drawable/btnnoticias"
        android:layout_alignBottom="@+id/imgbtnAfp"
        android:layout_alignEnd="@+id/gridLayout" />

</RelativeLayout>
