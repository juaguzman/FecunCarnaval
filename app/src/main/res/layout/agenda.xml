<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoagenda">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:id="@+id/listadoAgenda"
        android:layout_below="@+id/btnNag"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnNag"
        android:background="@drawable/btnnoticias"
        android:layout_marginStart="28dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnAag"
        android:background="@drawable/btngrande01"
        android:layout_alignBottom="@+id/btnNag"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="30dp" />
</RelativeLayout>