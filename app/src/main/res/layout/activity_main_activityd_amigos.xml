<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="example.juancarlos.com.fecuncarnaval.MainAmActivity"
    android:background="@drawable/fondovrd">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout2">

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna28"
            android:layout_row="0"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam28"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna29"
            android:layout_row="0"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam29"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna30"
            android:layout_row="1"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam30"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna31"
            android:layout_row="1"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam31"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna2"
            android:layout_row="2"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam2"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna3"
            android:layout_row="2"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam3"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna4"
            android:layout_row="3"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam4"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna5"
            android:layout_row="3"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/dam5"
            android:layout_marginRight="5dp" />

    </GridLayout>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgBtnVtA"
        android:background="@drawable/btngrande01"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-10dp" />

    <ImageButton
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:id="@+id/imgBtna6"
        android:layout_row="5"
        android:layout_column="0"
        android:background="@drawable/dam6"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imgBtnVtA"
        android:layout_alignParentStart="true"
        android:layout_marginTop="380dp"
        android:layout_marginBottom="15dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnNoA"
        android:layout_alignBottom="@+id/imgBtnVtA"
        android:layout_alignParentEnd="true"
        android:background="@drawable/btnnoticias" />
</RelativeLayout>
