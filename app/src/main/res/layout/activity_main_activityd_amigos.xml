<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="example.juancarlos.com.fecuncarnaval.MainAmActivity"
    android:background="@drawable/fondop">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout2">

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna28"
            android:layout_row="0"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da28"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna29"
            android:layout_row="0"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da29"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna30"
            android:layout_row="1"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da30"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna31"
            android:layout_row="1"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da31"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna2"
            android:layout_row="2"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da2"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna3"
            android:layout_row="2"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da3"
            android:layout_marginRight="5dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna4"
            android:layout_row="3"
            android:layout_column="0"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da4"
            android:layout_marginLeft="2dp" />

        <ImageButton
            android:layout_width="180dp"
            android:layout_height="84dp"
            android:id="@+id/imgBtna5"
            android:layout_row="3"
            android:layout_column="2"
            android:contentDescription="@string/btn28"
            android:background="@drawable/da5"
            android:layout_marginRight="5dp" />

    </GridLayout>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnCap"
        android:background="@drawable/btngrande01"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-10dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnNap"
        android:layout_alignBottom="@+id/imgbtnCap"
        android:layout_alignParentEnd="true"
        android:background="@drawable/btnnoticias" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgbtnAap"
        android:background="@drawable/agenda"
        android:layout_alignBottom="@+id/imgbtnCap"
        android:layout_alignParentStart="true" />
</RelativeLayout>
