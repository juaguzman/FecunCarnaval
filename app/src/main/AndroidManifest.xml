        <activity android:name=".MainFamActivity" >
        </activity>
        <activity android:name=".MainAmActivity" >
        </activity>
        <activity android:name=".MainActivityAmigos" >
        </activity>
        <activity android:name=".MainActivity_votacion" >
        </activity>
        <activity android:name=".MainActivityNoticias" >
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <provider
            android:name="com.facebook.FacebookContentProvider"
            android:authorities="com.facebook.app.FacebookContentProvider964526296921840"
            android:exported="true" />

        <activity android:name=".MainActivityFtoVt" >
        </activity>
        <activity android:name=".MainActivityAgenda" >
        </activity>
    </application>

</manifest>
